footer
  position absolute
  bottom 0
  left 0
  width 100%
  height 50px;
  background-color #f8f8f8
  border-top 1px solid #e7e7e7

footer .navbar
  margin 0
  padding 0

ul.navbar-footer
  margin 0
  width 100%
  height 50px
  position relative

ul.navbar-footer li
  display table-cell
  text-align center
  height 50px
  width 1%

ul.navbar-footer li a 
  padding 0
  height 100%

ul.navbar-footer li a i
  margin-top 12px

